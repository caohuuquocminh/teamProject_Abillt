	<section class="product">
          <h2><%= link_to product.name, store_product_path(product) %></h2>
          <ul><%= product.rating %></ul>
          <ul><%= number_to_currency product.price %></ul>
          <td><%= image_tag product.image.url(:thumb) %></td>
        </dl>
        
        <td class="image-description">
          <%= image_tag product.image_url, :class => 'list-image' %>
        </td>

	</section>